function y =  nakljucni_vzorec(x,p)
%Funkcija za parameter dobi matriko "x" dimenzije m x n in odstotek "p".
%Vrne novo matriko "y", ki ima le še "p"% prvotnih stolpcev.
    dim = size(x);
    n = dim(2);
    k = round(n* (p/100)); % "p"% "n"-ja zaokroženih na celo število
    y = x(:,sort(randperm(n,k)));%randperm zgenerira naklju?ni vektor k celih števil med 0 in n brez pona
end
