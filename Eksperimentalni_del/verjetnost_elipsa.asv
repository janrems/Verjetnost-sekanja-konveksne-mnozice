function [p] = verjetnost_elipsa (xmax,ymax,n)

    [velika,mala,pmax] = elipsa(xmax,ymax);
    A = vrni_tocke(n,pmax);
    for i = 1:n
        
        plot(A(i,[1 3]),A(i,[2 4]))
        hold on
    end
    plot(velika(:,1), velika(:,2))
    hold on
    plot(mala(:,1), mala(:,2))

    hold off
    
    [n_pres_v, A_nova] = presecisca(A,velika)
    length(A_nova(:,1))
    
    [n_pres_m] = presecisca_majhna(A_nova,majhna)
    
    
    
end